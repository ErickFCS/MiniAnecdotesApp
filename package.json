{
  "name": "anecdotes",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev:f": "vite Frontend",
    "dev:q": "vite ReactQueryFrontend",
    "dev:r": "vite ReactReduxFrontend",
    "build": "pnpm build:f && pnpm build:q && pnpm build:r",
    "build:f": "pnpm build:f:client && pnpm build:f:ssr",
    "build:f:client": "vite build Frontend --outDir ../dist/client/Frontend",
    "build:f:ssr": "vite build Frontend --outDir ../dist/server/Frontend --ssr src/entry-server.jsx",
    "build:q": "pnpm build:q:client && pnpm build:q:ssr",
    "build:q:client": "vite build ReactQueryFrontend --outDir ../dist/client/ReactQueryFrontend",
    "build:q:ssr": "vite build ReactQueryFrontend --outDir ../dist/server/ReactQueryFrontend --ssr src/entry-server.jsx",
    "build:r": "pnpm build:r:client && pnpm build:r:ssr",
    "build:r:client": "vite build ReactReduxFrontend --outDir ../dist/client/ReactReduxFrontend",
    "build:r:ssr": "vite build ReactReduxFrontend --outDir ../dist/server/ReactReduxFrontend --ssr src/entry-server.jsx",
    "lint:fix": "pnpm lint:b --fix && pnpm lint:f --fix && pnpm lint:q --fix && pnpm lint:r --fix",
    "lint": "pnpm lint:b && pnpm lint:f && pnpm lint:q && pnpm lint:r",
    "lint:b": "eslint -c ./eslint.config.js .",
    "lint:f": "eslint -c ./Frontend/eslint.config.js Frontend",
    "lint:q": "eslint -c ./ReactQueryFrontend/eslint.config.js ReactQueryFrontend",
    "lint:r": "eslint -c ./ReactReduxFrontend/eslint.config.js ReactReduxFrontend"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "bootstrap": "^5.3.8",
    "express": "^5.2.1",
    "react": "^19.2.4",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.4",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.3.0",
    "vite": "^7.3.1"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
