{
  "name": "anecdotes",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "build": "pnpm build:c && pnpm build:s",
    "build:c": "pnpm build:f && pnpm build:q && pnpm build:r",
    "build:s": "pnpm build:fs && pnpm build:qs && pnpm build:rs",
    "lint": "pnpm lint:f && pnpm lint:q && pnpm lint:r",
    "lintFix": "pnpm lint:f --fix && pnpm lint:q --fix && pnpm lint:r --fix",
    "dev:f": "vite Frontend",
    "build:f": "vite build Frontend --outDir ../dist/client/Frontend",
    "build:fs": "vite build Frontend --outDir ../dist/server/Frontend --ssr src/entry-server.jsx",
    "lint:f": "eslint -c Frontend/eslint.config.js Frontend",
    "preview:f": "vite preview Frontend",
    "dev:q": "vite ReactQueryFrontend",
    "build:q": "vite build ReactQueryFrontend --outDir ../dist/client/ReactQueryFrontend",
    "build:qs": "vite build ReactQueryFrontend --outDir ../dist/server/ReactQueryFrontend --ssr src/entry-server.jsx",
    "lint:q": "eslint -c ReactQueryFrontend/eslint.config.js ReactQueryFrontend",
    "preview:q": "vite preview ReactQueryFrontend",
    "dev:r": "vite ReactReduxFrontend",
    "build:r": "vite build ReactReduxFrontend --outDir ../dist/client/ReactReduxFrontend",
    "build:rs": "vite build ReactReduxFrontend --outDir ../dist/server/ReactReduxFrontend --ssr src/entry-server.jsx",
    "lint:r": "eslint -c ReactReduxFrontend/eslint.config.js ReactReduxFrontend",
    "preview:r": "vite preview ReacReduxyFrontend",
    "serve": "json-server --watch db.json --port 3001"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "@tanstack/react-query": "^5.66.10",
    "axios": "^1.8.1",
    "bootstrap": "^5.3.3",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@stylistic/eslint-plugin-js": "^4.1.0",
    "@types/json-server": "^0.14.8",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "json-server": "0.17.4",
    "vite": "^6.2.0"
  }
}